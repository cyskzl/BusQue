{
    "name": "mgdigital/busque",
    "autoload": {
        "psr-4": {
            "MGDigital\\BusQue\\": "src",
            "MGDigital\\BusQue\\Features\\": "features"
        }
    },
    "require": {
        "php": ">=7.0",
        "league/tactician": "~1.0",
        "predis/predis": "~1.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "phpspec/phpspec": "~2.5",
        "phpunit/phpunit": "~4.3",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "suggest": {
        "rybakit/msgpack": "For serialization to MessagePack format",
        "jms/serializer": "For serialization to json or XML",
        "symfony/console": "To run the console commands"
    },
    "config": {
        "bin-dir": "bin"
    }
}
